{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "typeRoots": ["./typings", "node_modules/@types"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "../tests-out",
    "baseUrl": "common",
    "paths": {
      "@/*": ["*"],
      "@pages/*": ["pages/*"],
      "@params/*": ["params/*"],
      "@utils/*": ["../utils/*"],
      "@fixtures": ["../fixtures.ts"]
    }
  },
  "include": ["**/*.ts", ".ts"],
  "exclude": ["node_modules"]
}
